<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="JuegoUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:JuegoUI"
    xmlns:pages="clr-namespace:JuegoUI.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="JuegoUI">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent Title="Game" ContentTemplate="{DataTemplate pages:GamePage}" Route="GamePage" />


</Shell>
